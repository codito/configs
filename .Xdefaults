!! defaults for XMessage
xmessage*maxWidth:  400
xmessage*maxHeight: 200

!! defaults for xterm
xterm*foreground:   white
xterm*background:   black
xterm*color0:      black
xterm*color1:      !40e0d0
xterm*color2:      !87cefa
xterm*color3:      !ffff00
xterm*color4:      !d1eeee
xterm*color5:      !eed8ae
xterm*color6:      !b4eeb4
xterm*color7:      gray
xterm*color8:      gray40
