xterm*color11:      !fff796
xterm*color12:      !00b2ee
xterm*color13:      !cf9ebe
xterm*color14:      !71bebe
xterm*color15:      white

! Xft
xft*dpi:	96
xft*antialias:	true
xft*hinting:	full

!! urxvt config
urxvt*termName: rxvt
! colors
urxvt*background:  rgba:0000/0000/0000/dddd
!urxvt*background:  #000000
urxvt*foreground:  #FFFFFF

! scroll
urxvt*secondaryScroll:  true
urxvt*scrollBar_right:	false
urxvt*scrollBar:	false

! borders
urxvt*borderWidth:	0
urxvt*borderLess:	false
urxvt*externalBorder:   0
urxvt*internalBorder:   0

! transparency
!urxvt*inheritPixmap:	true
!urxvt*backgroundPixmap: "/home/arun/pics/Wallpapers/clean/55700-Kore - Default.png"
!urxvt*geometry:		140x60
!urxvt*tintColor:        black
urxvt*fadeColor:        black
urxvt*fading:           1
urxvt*depth:            32
urxvt*shading:		20
urxvt*transparency:	true

! cursor and pointer
urxvt*cursorColor:      lightgreen
urxvt*cursorBlink:      false
urxvt*pointerBlank:	true

! font
!urxvt*font:		-*-dina-medium-r-normal-*-12-*-*-*-*-*-*-*
!urxvt*lineSpace:        1
!urxvt*font:             xft:terminus:size=5;antialias=true
urxvt*font:             xft:inconsolata:size=10;antialias=true
!urxvt*font:             xft:ori1Uni:size=8;antialias=true
!urxvt*font:             xft:DejaVu Sans Mono:size=10:antialias=true
!urxvt*font:		 xft:Bitstream Vera Sans Mono:size=9:antialias=true
!urxvt*boldFont:         xft:inconsolata:size=10

! Misc
! urxvt*loginShell:       true
urxvt*saveLines:        10000
urxvt.matcher.button:       1
urxvt.matcher.pattern.1:    \bwww\.[\w-]\.[\w./?&@!-]*[\w/-]
urxvt*urlLauncher:        opera -newtab
urxvt.perl-ext-common:      default,matcher


